# Mediawiki runJobs cron:
*/20 * * * * /usr/local/bin/php /var/www/html/maintenance/runJobs.php --quiet --maxjobs=200 --maxtime=300 --memory-limit=128M > /proc/1/fd/1 2>/proc/1/fd/2

# Semantic mediawiki maintenance:
15 5 * * * /usr/local/bin/php /var/www/html/extensions/SemanticMediaWiki/maintenance/rebuildData.php --quiet --shallow-update > /proc/1/fd/1 2>/proc/1/fd/2
15 6 * * * /usr/local/bin/php /var/www/html/extensions/SemanticMediaWiki/maintenance/disposeOutdatedEntities.php --quiet > /proc/1/fd/1 2>/proc/1/fd/2
15 7 * * * /usr/local/bin/php /var/www/html/extensions/SemanticMediaWiki/maintenance/rebuildPropertyStatistics.php --quiet > /proc/1/fd/1 2>/proc/1/fd/2
15 8 * * * /usr/local/bin/php /var/www/html/extensions/SemanticMediaWiki/maintenance/rebuildConceptCache.php --quiet --update --create > /proc/1/fd/1 2>/proc/1/fd/2
15 9 * * SUN /usr/local/bin/php /var/www/html/extensions/SemanticMediaWiki/maintenance/rebuildData.php --quiet -d 100 > /proc/1/fd/1 2>/proc/1/fd/2
30 5 * * SUN /usr/local/bin/php /var/www/html/extensions/SemanticMediaWiki/maintenance/setupStore.php --quiet --skip-import > /proc/1/fd/1 2>/proc/1/fd/2
15 3 1 * * /usr/local/bin/php /var/www/html/extensions/SemanticMediaWiki/maintenance/removeDuplicateEntities.php --quiet  > /proc/1/fd/1 2>/proc/1/fd/2

